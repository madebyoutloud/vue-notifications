{
  "name": "@outloud/vue-notifications",
  "title": "Vue Notifications",
  "description": "A library for displaying notification/toast in Vue 3.",
  "private": false,
  "version": "2.0.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": {
      "import": "./dist/style.css"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "imports": {
    "~/*": "./src/*.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://git.outloud.dev/outloud/libraries/vue-notifications"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode lib",
    "test": "vitest run",
    "test:dev": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "prepack": "pnpm build",
    "release": "pnpm build && release-it"
  },
  "devDependencies": {
    "@iconify/vue": "^4.1.2",
    "@outloud/eslint-config-vue": "^1.1.2",
    "@unocss/reset": "^0.62.3",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^15.7.2",
    "sass-embedded": "^1.77.8",
    "sass-loader": "^16.0.1",
    "unocss": "^0.62.3",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^4.0.3",
    "vitest": "^2.0.5",
    "vue": "^3.4.38",
    "vue-tsc": "^2.1.2"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  }
}
