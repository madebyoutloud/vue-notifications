{
  "extends": "@antfu",
  "rules": {
    "vue/multi-word-component-names": "off",
    "vue/component-tags-order": ["error", {
      "order": ["template", "script", "style"]
    }],
    "@typescript-eslint/consistent-type-definitions": ["off"],
    "curly": ["error", "all"]
  },
  "ignorePatterns": ["**/*.md"]
}